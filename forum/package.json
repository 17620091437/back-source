    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.5",
    "mysql2": "^1.6.4",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^5.3.0"
  }
